      "\n",
      "SH = np.zeros((len(ccThresh),len(coThresh)))\n",
      "\n",
      "for i, cc in enumerate(ccThresh):\n",
      "    for j, co in enumerate(coThresh):\n",
      "        pnl = backtest(ohlc, ccThresh=cc, coThresh=co)\n",
      "        SH[i,j] = sharpe(pnl)\n",
      "        \n",
      "pcolor(coThresh,ccThresh, SH)\n",
      "xlabel('opening gap [%]')\n",
      "ylabel('previous day change [%]');\n",
      "colorbar();"
